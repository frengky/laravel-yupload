{
    "name": "frengky/yupload",
    "description": "Laravel package for easy file uploads maintenance",
    "keywords": ["laravel", "model", "upload", "database"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/frengky/laravel-yupload",
    "authors": [
        {
            "name": "Frengky",
            "email": "frengky.lim@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "6.*",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.4",
        "orchestra/database": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Frengky\\Yupload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frengky\\Yupload\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Frengky\\Yupload\\UploadServiceProvider"
            ]
        }
    }
}
